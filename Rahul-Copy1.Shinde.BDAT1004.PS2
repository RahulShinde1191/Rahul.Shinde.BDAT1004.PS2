    "def binary(n):\n",
    "    if n == 0:\n",
    "        return \"0\"\n",
    "    elif n == 1:\n",
    "        return \"1\"\n",
    "    else:\n",
    "        return binary(n//2) + str(n%2)\n",
    "print(binary(1))\n",
    "print(binary(3))"
